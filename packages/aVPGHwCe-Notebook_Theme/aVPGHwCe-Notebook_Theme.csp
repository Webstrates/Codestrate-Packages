}

html[transient-theme="Notebook"] .awesomplete > ul > li[aria-selected="true"] {
	background: rgba(var(--color-accent-rgb), 0.5);
}

html[transient-theme="Notebook"] .awesomplete li[aria-selected="true"] mark {
